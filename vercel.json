{
    "routes": [
      {
        "src": "/assets/(.*)",
        "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
      },
      {
        "src": "/(.*)\\.(js|css|json|ico|png|jpg|svg|woff2?|ttf|otf|eot|wasm)$",
        "dest": "/dist/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "status": 200
      }
    ]
  }
  